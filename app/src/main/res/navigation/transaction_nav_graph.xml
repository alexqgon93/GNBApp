<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/transaction_nav_graph"
    app:startDestination="@id/transactionsFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/transactionsFragment"
        android:name="com.gnb.gnbapp.transactions.view.TransactionsFragment"
        android:label=" "
        tools:layout="@layout/fragment_transactions">

        <argument
            android:name="product"
            app:argType="string"
            app:nullable="false" />

        <argument
            android:name="totalPurchased"
            app:argType="string"
            app:nullable="false" />

        <argument
            android:name="list"
            app:argType="com.gnb.gnbapp.data.model.Transactions"
            app:nullable="false" />
        
    </fragment>

</navigation>